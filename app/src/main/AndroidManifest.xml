<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vikasperaka.roundupv4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateEvent"
            android:screenOrientation="portrait"/>
        <activity android:name=".JoinEvent"
            android:screenOrientation="portrait"/>
        <activity android:name=".Results"
            android:screenOrientation="portrait"/>
        <activity android:name=".JoinCalendar"
            android:screenOrientation="portrait"/>
        <activity android:name=".JoinUser"
            android:screenOrientation="portrait"/>
        <activity android:name=".Confirmation"
            android:screenOrientation="portrait"/>
        <activity android:name=".CreateCalendar"
            android:screenOrientation="portrait"/>
        <activity android:name=".CodeCreate"
            android:screenOrientation="portrait"/>
        <activity android:name=".CalendarScreen"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".UserInputData"
            android:label="@string/title_activity_user_input_data"
            android:theme="@style/AppTheme.NoActionBar">
            android:screenOrientation="portrait"
        </activity>
    </application>

</manifest>