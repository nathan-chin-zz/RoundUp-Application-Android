<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vikasperaka.roundupv4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateEvent" />
        <activity android:name=".JoinEvent" />
        <activity android:name=".Results" />
        <activity android:name=".JoinCalendar" />
        <activity android:name=".JoinUser" />
        <activity android:name=".Confirmation" />
        <activity android:name=".CreateCalendar" />
        <activity android:name=".CodeCreate" />
        <activity android:name=".CalendarScreen" />
        <activity
            android:name=".UserInputData"
            android:label="@string/title_activity_user_input_data"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>